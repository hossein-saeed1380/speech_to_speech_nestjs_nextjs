generator client {
  provider        = "prisma-client"
  output          = "../src/generated/prisma"
  moduleFormat    = "cjs"
}

datasource db {
  provider        = "postgresql"
}

model AiFeatures {
  id        Int         @default(autoincrement()) @id
  firstname String?      
  lastname  String?
  email     String?
  phone     String           
}
