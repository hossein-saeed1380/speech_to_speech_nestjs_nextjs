generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model AiFeatures {
  id        Int     @id @default(autoincrement())
  firstname String?
  lastname  String?
  email     String?
  phone     String
  input     String   @default("")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt @default(now())
}
